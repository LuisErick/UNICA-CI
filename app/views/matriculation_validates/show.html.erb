<%- model_class = MatriculationValidate -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:matriculation_id) %>:</strong></dt>
  <dd><%= @matriculation_validate.matriculation_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:institution) %>:</strong></dt>
  <dd><%= @matriculation_validate.institution %></dd>
  <dt><strong><%= model_class.human_attribute_name(:state) %>:</strong></dt>
  <dd><%= @matriculation_validate.state %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              matriculation_validates_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_matriculation_validate_path(@matriculation_validate), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              matriculation_validate_path(@matriculation_validate),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
