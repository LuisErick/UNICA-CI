<%= form_for @package, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :teacher_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :teacher_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :schedule_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :schedule_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :room_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :room_id, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                packages_path, :class => 'btn' %>
  </div>
<% end %>
