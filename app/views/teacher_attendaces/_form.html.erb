<%= form_for @teacher_attendace, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :package_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :package_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :state, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :state, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :arrival_time, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :arrival_time, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :dismissal_time, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :dismissal_time, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :session_number, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :session_number, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :day, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :day, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                teacher_attendaces_path, :class => 'btn' %>
  </div>
<% end %>
