<%= form_for @pre_mariculation_communicative, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :pre_matriculation_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :pre_matriculation_id, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :level_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :level_id, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                pre_mariculation_communicatives_path, :class => 'btn' %>
  </div>
<% end %>
