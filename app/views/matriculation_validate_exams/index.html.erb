<%- model_class = MatriculationValidateExam -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:matriculation_validate) %></th>
      <th><%= model_class.human_attribute_name(:grade) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @matriculation_validate_exams.each do |matriculation_validate_exam| %>
      <tr>
        <td><%= link_to matriculation_validate_exam.id, matriculation_validate_exam_path(matriculation_validate_exam) %></td>
        <td><%= matriculation_validate_exam.matriculation_validate %></td>
        <td><%= matriculation_validate_exam.grade %></td>
        <td><%=l matriculation_validate_exam.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_matriculation_validate_exam_path(matriculation_validate_exam), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      matriculation_validate_exam_path(matriculation_validate_exam),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_matriculation_validate_exam_path,
            :class => 'btn btn-primary' %>
