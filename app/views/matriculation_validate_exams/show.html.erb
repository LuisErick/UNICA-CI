<%- model_class = MatriculationValidateExam -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:matriculation_validate) %>:</strong></dt>
  <dd><%= @matriculation_validate_exam.matriculation_validate %></dd>
  <dt><strong><%= model_class.human_attribute_name(:grade) %>:</strong></dt>
  <dd><%= @matriculation_validate_exam.grade %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              matriculation_validate_exams_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_matriculation_validate_exam_path(@matriculation_validate_exam), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              matriculation_validate_exam_path(@matriculation_validate_exam),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
