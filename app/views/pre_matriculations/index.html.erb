<%- model_class = PreMatriculation -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:person_id) %></th>
      <th><%= model_class.human_attribute_name(:curse_type_id) %></th>
      <th><%= model_class.human_attribute_name(:languaje_id) %></th>
      <th><%= model_class.human_attribute_name(:state) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @pre_matriculations.each do |pre_matriculation| %>
      <tr>
        <td><%= link_to pre_matriculation.id, pre_matriculation_path(pre_matriculation) %></td>
        <td><%= pre_matriculation.person_id %></td>
        <td><%= pre_matriculation.curse_type_id %></td>
        <td><%= pre_matriculation.languaje_id %></td>
        <td><%= pre_matriculation.state %></td>
        <td><%=l pre_matriculation.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_pre_matriculation_path(pre_matriculation), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      pre_matriculation_path(pre_matriculation),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_pre_matriculation_path,
            :class => 'btn btn-primary' %>
