<h1>Listing matricualtion_validates</h1>

<table>
  <thead>
    <tr>
      <th>Matriculation</th>
      <th>Institution</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @matricualtion_validates.each do |matricualtion_validate| %>
      <tr>
        <td><%= matricualtion_validate.matriculation_id %></td>
        <td><%= matricualtion_validate.institution %></td>
        <td><%= link_to 'Show', matricualtion_validate %></td>
        <td><%= link_to 'Edit', edit_matricualtion_validate_path(matricualtion_validate) %></td>
        <td><%= link_to 'Destroy', matricualtion_validate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Matricualtion validate', new_matricualtion_validate_path %>
